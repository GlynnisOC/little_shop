<h1>Update Your Addresses</h1>

<%= form_for @address, url: edit_address_path do |f| %>
  <%= f.label :address %>
  <%= f.text_field :address %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :state %>
  <%= f.text_field :state %>

  <%= f.label :zip %>
  <%= f.text_field :zip %>

  <%= f.submit "Update Addresses" %>

<% end %>
